{
  "name": "express-mysql",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:devoffisis/express-mysql-nginx.git",
  "author": "Yoshitsugu Sekine <yoshitsugu.sekine@offisis.co.jp>",
  "license": "",
  "private": true,
  "scripts": {
    "dev": "run-s -l prisma:migrate:dev prisma:seed start:dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --exit-child src/index.ts",
    "check-types": "tsc --noEmit",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint src/**/*.ts",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -l -c fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix src/**/*.ts",
    "fix:prettier": "prettier --write .",
    "preinstall": "npx typesync || :",
    "prisma:generate": "prisma generate",
    "prisma:generate:test": "DATABASE_URL='mysql://root:root@mysql:3306/mydb_test' prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:dev:test": "DATABASE_URL='mysql://root:root@mysql:3306/mydb_test' prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:seed:test": "DATABASE_URL='mysql://root:root@mysql:3306/mydb_test' prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:studio:test": "DATABASE_URL='mysql://root:root@mysql:3306/mydb_test' prisma studio",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "test": "DATABASE_URL='mysql://root:root@mysql:3306/mydb_test' NODE_ENV=test jest"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/eslint": "^8.4.8",
    "@types/express": "^4.17.14",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.0",
    "@types/nodemon": "^1.19.2",
    "@types/prettier": "^2.7.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.1.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "typesync": "^0.9.2"
  },
  "dependencies": {
    "@prisma/client": "4.4.0",
    "bcrypt": "^5.1.0",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  }
}
